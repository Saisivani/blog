<%- include('./parts/head') %>
<table border="1">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Body</th>
        </tr>
    </thead>
    <tbody>
    <% posts.forEach((post) => { %>
        <tr>
            <td><%= post.id%></td>
            <td><%= post.title%></td>
            <td><%= post.body%></td>
        </tr>
    <% }) %>
    </tbody>
</table>
<%- include('./parts/tail') %>